# +mkmake+ -- Everything after this line is automatically generated

EXECUTABLES = pieln10 coutcerr hwpf ints1 ints2 printchars ssn-reader weirdints

all: $(EXECUTABLES)

clean:
	rm -f core *.o $(EXECUTABLES) a.out

.cpp.cpp:
	g++ $(CFLAGS) -c $*.cpp


pieln10: pieln10.cpp
	g++ $(CFLAGS) -o pieln10 pieln10.cpp

coutcerr: coutcerr.cpp
	g++ $(CFLAGS) -o coutcerr coutcerr.cpp

hwpf: hwpf.cpp
	g++ $(CFLAGS) -o hwpf hwpf.cpp

ints1: ints1.cpp
	g++ $(CFLAGS) -o ints1 ints1.cpp

ints2: ints2.cpp
	g++ $(CFLAGS) -o ints2 ints2.cpp

printchars: printchars.cpp
	g++ $(CFLAGS) -o printchars printchars.cpp

ssn-reader: ssn-reader.cpp
	g++ $(CFLAGS) -o ssn-reader ssn-reader.cpp

weirdints: weirdints.cpp
	g++ $(CFLAGS) -o weirdints weirdints.cpp
